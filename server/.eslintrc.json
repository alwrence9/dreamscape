{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prefer-const": "warn",
        "array-callback-return": "error",
        "eqeqeq": "warn",
        "no-const-assign": "error",
        "no-cond-assign": "error",
        "no-unreachable": "error"
    }
}
